/返回首页按钮样式
.homeDiv {
  position: absolute;
  top: 46px;
  left: 20px;
  z-index: 50;
  width: 32px;
  height: 32px;
  
	background-color: #fff;
	border-radius: 6px;
  text-align: center;
  border: 1px solid #000;
  .icon-shouye {
    color: #444;
    font-size: 20px;
    line-height: 32px;
    text-decoration: none;
   
  }
  //去除点击后的虚线边框
  a:focus {
    outline-style:none;
    -moz-outline-style: none;
  }
  &:hover {
    background: #eee;
    .iconfont{
      color: #03b1a7;
    } 
  }
}


.side_action_bar_placeholder {
  width: 100%;
  position: absolute;
  top: 0;
  right: 0;
  left: 0;
  line-height: $MapActionBarBgHeight;
  height:$MapActionBarBgHeight;
  color: #444;
  font-family: arial;
  z-index: 2;
  background-color: rgba(47,50,57,.9);
  
  

  
  // 地图顶部描述字体样式
  .Text-Wraper {
    padding-left: 10px;
    color: $MapActionBarTextColor;
    letter-spacing: 0.7px;

    .orange {
      color: $MapActionBarTextActiveColor;
    }
    & > span{
      padding: 0 10px;

      &:after{
        content:'';
        display: inline-block;
        width: 1px;
        height: 21px;
        opacity: 0.9;
        background-color: #2f3239;
	      box-shadow: 1px 0px 0px 0px #5b6479;
        vertical-align: middle;
      }
    }
    & span:last-child{
      padding-right:10px;
    }
  }

  .Map_Action_Bar_Search{
    padding-left: $leftBarWidth; 
  }

  .row {
    align-items: center;
  }
}

// 顶部查询样式
.Map_Action_Bar_Search {
  width: 100%;
  justify-content: space-between;
  align-item: center;
  font-size: $fontSizeNormal;
  opacity: $MapActionBarOpacity;
  line-height: 42px;
  height: 42px;

  .search-factor{

    input{
      color: #fff;
      width: 60px;
      padding:0 26px 0 4px;
	    height: 27px;
      line-height:28px;
	    background-color: #5a5f6b;
	    border: solid 1px #5a5f6b;
      border-radius: 2px  0 0 2px;
    }
    &:focus,
    &.is-active {
      outline: none;
      border-color: #5a5f6b;
    }
    .el-input__suffix{
      .el-icon-arrow-up:before{
        content: '';
        height: 0px;
        width: 0px;
        display: inline-block;
        border-bottom: 6px solid #fff;
        border-left: 5px transparent solid;
        border-right: 5px transparent solid;
        vertical-align: middle;
        transform: scale(0.9);
      }
      right: -1px;
    }
    .el-input__icon{
      line-height: normal;
    }
  }
  .el-select .el-input__inner:focus{
    outline: none;
    border-color: #5a5f6b;
  }

  .input-Wraper{
    width: 160px;
    input{
      color: #fff;
      height: 27px;
      background-color: #333742;
      border: solid 0.5px #5a5f6b;
      border-radius: 0;
      
    }
    .el-input-number{
      height: auto;
    }

    &.between{
      display:flex;
      flex-direction:row;
      justify-content: space-between;
      input{
        flex: 0 0 50%;
        padding-left:10px;
        padding-right: 10px;
        text-align: left;
      }
      
      
      .el-input-number__decrease,
      .el-input-number__increase{
        width: 20px;
        background: transparent;
        height: 12px;
        line-height: 12px;
        color: #fff;
        border:none;
        &:hover{
          color:#409eff;
        }
      }
    }
  }

  .search {
    margin-right: 20px;
    width: 26px;
    height: 26px;
    line-height: 26px;
    text-align: center;
    color: #fff;
    border: solid 0.5px #5a5f6b;

    .iconfont {
      font-size: 18px;
      line-height: 26px;
    }
  }
}



// 图层控制弹窗样式
.Map_Action_Bar_Layer_Win {
  height: auto;
  border: 1px solid #3b3d44;
  background: transprant;;
  position: absolute;
  top: 41px;
  right: 0;
  box-shadow: -1.5px 1.3px 6px 0px rgba(179, 179, 179, 0.38);
  font-size: $fontSizeNormal;

  .el-input__icon{
    line-height: 28px;
  }
  .Win_Title{
    display: flex;
    flex-direction:row;
    justify-content: space-between;
    height: $MapActionBarBgHeight;
    line-height: $MapActionBarBgHeight;
    font-size: 14px;
    background:#fff;
    color:#199cdd;
    padding: 0 4px;
    .text{
      &:before{
        margin-right: 10px;
        content:'';
        display: inline-block;
        width: 4px;
	      height: 10px;
	      background-color: #0080c1;
        border: 3px solid  #98cce5;
        vertical-align: sub;
      }
    }
    .icon{
      height: $MapActionBarBgHeight;
      cursor:pointer;
      font-size:$fontSizeNormal
      text-align:center;
      width:30px;
      z-index: 9;
      color:#6a8694;
    }
  }
  // 图层弹窗样式
  .TuCeng_List {
    width: 140px;
    height: $MapActionBarWraperHeight - $MapActionBarBgHeight;
    color: #707070;
    position:relative;
    background:#fff;
    

    .el-scrollbar {
      height: 100%;

      .el-scrollbar__wrap {
        padding-bottom: 20px;
        .el-scrollbar__view{
          overflow: visible;
          width: 100%;
          border: 1px saddlebrown;
        }
      }
    }

    // 图层弹窗大图片列表
    .List_Item {
      border-top: solid #e6e6e3 1px;
      padding: 10px;
      .Win_Img_List_Item {
        height: 72px;
        border: 1px #0091da solid;
        position: relative;
        text-align: center;

        &.active span{
          opacity: 1;
          background-color: #0091da;
        }

        img {
          width: 100%;
          height: 100%;
        }

        span {
          width: 70px;
          height: 25px;
          line-height: 25px;
          text-align: center;
          position: absolute;
          background-color: rgba(47,50,57,.5);
          color: #fff;
          bottom: 0;
          right: 0;
          cursor: pointer;
        }
      }

      .Win_Text_List_Item {
        padding: 4px 0;
        font-size: $fontSizeNormal;
        line-height: 24px;
        color: #333;
        overflow: hidden;

        .el-slider {
          width: 90%;

          .el-slider__runway {
            margin: 0 0;
            border-radius: 8px;
            background-color: transparent;
            border: 1px solid  #00c3aa;
            
          }
          .el-slider__button{
            width: 10px;
	          height: 10px;
            border-color:#02b0d2;;
            background:#02b0d2;
          }
          .el-slider__bar{
            border-radius: 3px;
            background-image: linear-gradient(-83deg, #01aed5 0%, #00c3aa 100%);
          }
        }
        .tuceng_title{
          float:left;
        }
        .Win_Range_Value {
          float: right;
          color: #0091da;
        }
      }
    }

    // 图层弹窗多选列表
    .She_Shi_Layer {
      border-top: solid #e6e6e3 1px;
      padding: 10px 0;
      padding-right: 10px;
      
      .el-checkbox {
        with: 100%;
        display: block;
        color: #fff;
        line-height: 28px;
        .el-checkbox__input.is-checked .el-checkbox__inner {

        }
        .el-checkbox__label {
          padding-left: 5px;
        }
      }
      .el-tree{
        background:transparent;
        color:#333;
      } 
      .expanded.el-tree-node__expand-icon.el-icon-caret-right{
        color: #0091da;
      }
      .el-tree-node__content:hover{
        background:transparent;
      }
      .el-tree-node:focus>.el-tree-node__content{
        background:transparent;
      }
      .el-tree-node,
      .el-tree-node__content{
        width: 100%;
        overflow: visible;
      }
      .el-tree-node__content{
        flex-wrap: nowrap
      }
      .el-tree-node > .el-tree-node__children{
        overflow: visible;
        }
    }
  }


  // 图例弹窗样式
  .Tuli_List {
    height: $MapActionBarWraperHeight - $MapActionBarBgHeight;
    margin: 0;
    width: 140px;
    text-align: center;
    color: #333333;
    position:relative;
    background:#fff;

    .el-scrollbar {
      height: 100%;

      .el-scrollbar__wrap {
       padding-bottom: 20px;
      }
    }
    
    .Tuli-Pipe_Item{
      margin-left: 10px;
      margin-right: 10px;
      border-bottom: 1px solid #e8e8e6;
      height: 50px;
      .pipe-name{
        text-align: right;
        height: 27px;
        white-space: nowrap;
      }
      .pipe-icon{
        display: inline-block;
        vertical-align: top;
        height: 6px;
        // background-color: #663399;
        // border: 1px  solid #181919;
        background-size: contain;
        width: 100%;
        margin: auto;
        box-sizing: border-box;
        margin-top: 8px;
        background-size: 400%;
        background-position: center;
      }
    }
    .Tuli_List_Item {
      list-style: none;
      vertical-align: middle;
      padding-left: 10px;
      padding-right: 10px;
      height: 100%;
      line-height: 100%;
      
      span {
        overflow:hidden;
        display: inline-block;
        width: 50%;
        line-height: 35px;
        height: 35px;
        box-sizing: border-box;

        img{
          width: 20px;
          height: 20px;
          vertical-align: middle;
        }
      }
    }
  }

  .print_List{
    height: auto;
    position:relative;
    background:#fff;
    .el-scrollbar {
      height: 100%;

      .el-scrollbar__wrap {
        overflow-x: hidden;
      }
    }
  }
}

// 右侧地图操作图标样式
.Map_Action_Bar_container {
  display:flex;
  flex-direction:row-reverse;
  justify-content: end;
   line-height: 42px;

  .action_item {
    height: 34px;
    width: 40px;
    line-height: 34px;
    color: $MapActionBarTextColor;
	  opacity: 0.7;
    text-align: center;
    cursor: pointer;


    &:hover ,
    &.active{
      opacity:1;
    }

    .iconfont {
      font-size: $MapActionBarIconSize;
      line-height: 34px;
    }
  }
}
.print_List{
  .el-scrollbar__wrap{
    margin-bottom: 0 !important;
  }
}
.print-wrapper{
    height: 100%;
    background: #fff;
    width: 400px;
    width: 300px;
    padding: 10px 20px 0 0;
}
//选择单位的popover样式
.Map_Action_Bar_popover{
  min-width: 72px;
  box-shadow: 0px 1px 0px 0px rgba(47, 50, 57, 0.14);
	border-radius: 2px;
	border: solid 1px #e1ecf5;
  padding: 0;
  font-size: 14px;
  .el-button {
    &:last-child{
      &:after{
        display: none;
      }
    }
    text-align: left;
    width: 100%;
    padding: 0;
    display: block;
    height:30px;
    border: none;
    margin-left: 0;
    padding-left: 10px;
    position: relative;
    
    &:after{
      content: '';
      display: inline-block;
      width: 100%;
      border: 1px dashed #e1ecf5;
      position: absolute;
      bottom: 0;
      left: 0;
    }
    &:focus, 
    &:hover{
      background-color: #fff;
    }
    &.active{
      background-color: #e1ecf5;
      color: #0091da;
    }

    

    

  }

}

#mapNode_zoom_slider{
  display:none;
}

