
myBtn(color = 2px, fontColor = #fff) {
  background: color;
  border-color: color;
  color: fontColor;
  border-radius: 2px;

  &:focus {
    border-color: color;
    background: color;
    color: #fff;
  }

  &:hover {
    opacity: 0.7;
  }
}

