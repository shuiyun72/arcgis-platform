.el-button {
  .iconfont {
    font-size: 12px;
  }
}

.el-button.my-tableout {
  background: transparent;
  color: #afbcc4;
  border-color: #58647b;
  border-radius: 2px;
  height: 30px;
  padding: 0 10px;

  .iconfont {
    color: #afbcc4;
    font-size: 16px;
    vertical-align: text-bottom;
    margin-right: 2px;
  }

  &.active {
    background: #53678e;
    border-color: #58647b;
  }

  &:hover, &:focus {
    background: #53678e;
    border-color: #58647b;
    color: #fff;

    .iconfont {
      color: #fff;
    }
  }
}

my-table-icon(color) {
  myBtn(color);
  width: 20px;
  height: 20px;
  padding: 0;

  .iconfont {
    font-size: 14px;

    &:before {
      margin-right: 0;
    }
  }
}

.el-button.my-table-del {
  my-table-icon(#bf4e49);
}

.el-button.my-table-edit {
  my-table-icon(#c7741c);
}

.el-button.my-table-detail {
  my-table-icon(#0b9fd0);
}

.my-add-point {
  background: transparent;
  border: none;
  color: #28b46d;

  &:hover, &:focus {
    background: transparent;
    border: none;
  }

  .text {
    text-decoration: underline;
  }

  .iconfont {
    text-decoration: none;
    font-size: 16px;
    vertical-align: top;
  }
}

.el-button.my-dialog-submit {
  width: 78px;
  height: 32px;
  line-height: 32px;
  color: #fefefe;
  background-image: linear-gradient(
    0deg,
    #0061da 0%,
    #0099e6 100%
  );
  border-radius: 2px;
  font-size: 16px;
  padding: 0;
}

.el-button.my-dialog-cancel {
  width: 78px;
  height: 32px;
  line-height: 32px;
  border: solid 1px #0091da;
  background: transparent;
  color: #0091da;
  font-size: 16px;
  border-radius: 2px;
  padding: 0;
}

.my-radio {
  margin-right: 10px;
  line-height: 30px;
}

